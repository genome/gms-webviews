<div class="well">
  <h4><%= genome_model_with_builds.name %></h4>
  <ul>
    <li>ID: <%= genome_model_with_builds.id %></li>
    <li>Subject Name: <%= genome_model_with_builds.subject.name %></li>
    <li>Subject Type: <%= genome_model_with_builds.subject_class_name %></li>
    <li>Username: <%= genome_model_with_builds.user_name %></li>
    <li>Created: <%= genome_model_with_builds.creation_date %></li>
  </ul>
  <div class="well">
    Inputs
    <ul>
      <% genome_model_with_builds.model_inputs.each do |input| %>
        <li><%= "#{input.name} - #{input.value_id}"%></li>
      <% end %>
    </ul>
    <div>
      <table class="table table-striped">
        <tr>
          <th>build</th>
          <th>scheduled</th>
          <th>completed</th>
          <th>data directory</th>
        </tr>
        <%= render partial: 'build/build_table_row', collection: genome_model_with_builds.builds %>
      </table>
    </div>
  </div>
</div>
