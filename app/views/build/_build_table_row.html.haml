%tr
  %td= link_span(build_table_row.id, build_path(build_table_row))
  %td= status_label_for_build(build_table_row)
  %td= build_table_row.master_event ? time_tag(build_table_row.master_event.date_scheduled.localtime) : ''
  %td= (build_table_row.master_event && build_table_row.master_event.date_completed) ? time_tag(build_table_row.master_event.date_completed.localtime) : ''
  %td= link_span 'data directory', "http://gscweb.gsc.wustl.edu/#{build_table_row.data_directory}", "icon-folder-open icon-white"

-if defined?(missing_inputs_for_build) && missing_inputs_for_build[build_table_row.build_id].keys.length != 0
  %tr
    %td
    %td
      %span{class: "label label-warning"}
        Inputs differ
    %td{colspan:"3"}
      =render_build_inputs(@delta_inputs_for_build[build_table_row.build_id])
