
<div class="well">
    <h1>Build <%= @build.build_id %></h1>
    <h3><%= @build.model.name %></h3>

    <br/>
    <%= render partial: 'build/summary', locals: { build: @build } %>

</div>

    <div class="span12" style="background-color: pink">
      <div class="page-header">
        <h2>Inputs</h2>
      </div>

        <table class="table">
            <tbody>
          <% @inputs.each do |i| %>
            <tr>
                <td><%= i.name %></td>
                <td><%= i.value_id %></td>
            </tr>
          <% end  %>
            </tbody>
        </table>
    </div>


