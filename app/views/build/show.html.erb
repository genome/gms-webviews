
<div class="page-header">
  <h3><%= @build.model.name %></h3>
  <h1><%= @build.build_id %> <small>build</small></h1>
</div>
<div class="well">
    <%= render partial: 'build/summary', locals: { build: @build } %>
</div>

<div class="span12">
  <div class="page-header">
    <h2>Inputs</h2>
  </div>

    <table class="table table-bordered">
        <tbody>
      <% @inputs.each do |i| %>
        <tr>
            <td><%= i.name %></td>
            <td>
                <%= render_value_for_model_or_build_input(i) %>
            </td>
        </tr>
      <% end  %>
        </tbody>
    </table>
</div>

<div class="span12">
    <div class="page-header">
        <h2>Workflow</h2>
    </div>
    not in postgres yet.
</div>


