
<div class="well">
    <h1>Build <%= @build.build_id %></h1>
    <h3><%= @build.model.name %></h3>

    <br/>
    <%= render partial: 'build/summary', locals: { build: @build } %>

</div>

<div class="span12">
  <div class="page-header">
    <h2>Inputs</h2>
  </div>

    <table class="table table-bordered">
        <tbody>
      <% @inputs.each do |i| %>
        <tr>
            <td><%= i.name %></td>
            <td><%= link_span("here", build_input_path(i)) %>
                <%= i.value_id %>
            </td>
        </tr>
      <% end  %>
        </tbody>
    </table>
</div>

<div class="span12">
    <div class="page-header">
        <h2>Workflow</h2>
    </div>

    <div class="span12"/>
https://imp.gsc.wustl.edu/viewajax/workflow/operation/instance/statuspopup.html?id=22955365
</div>


