.page-header
  %h3= @build.build_id
  %h1
    =@build.model_name
    %small build
.span12
  =render partial: 'shared/attribute_list', locals: { presenter: @build }

.span12
  .page-header
    %h2 Inputs
  =render partial: 'shared/attribute_list', locals: { presenter: @build.inputs }

.span12
  .page-header
    %h2 Workflow
  =render partial: 'workflow/workflow_table', locals: {workflow_instance: @build.workflow_instance}

=javascript_include_tag :builds